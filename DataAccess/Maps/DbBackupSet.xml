<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="DbBackupSetMap"   xmlns="http://ibatis.apache.org/mapping"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="SqlMap.xsd">
  <alias>
    <typeAlias alias="DbBackupSet" assembly="DataAccess.dll" type="DataAccess.Domain.DbBackupSet" />
  </alias>
  <resultMaps>
    <resultMap id="DbBackupSetResult" class="DbBackupSet">
      <result property="Id" column="ID" type="long" dbType="long"/>
      <result property="Cycle" column="Cycle" type="string" dbType="varchar"/>
      <result property="DayTime" column="DayTime" type="string" dbType="varchar"/>
      <result property="WeekDay" column="WeekDay" type="string" dbType="varchar"/>
      <result property="WeekTime" column="WeekTime" type="string" dbType="varchar"/>
      <result property="MonthDay" column="MonthDay" type="long" dbType="long"/>
      <result property="MonthTime" column="MonthTime" type="string" dbType="varchar"/>
      <result property="CreateUser" column="CreateUser" type="string" dbType="varchar"/>
      <result property="CreateTime" column="CreateTime" type="DateTime" dbType="DateTime"/>
      <result property="UpdUser" column="UpdUser" type="string" dbType="varchar"/>
      <result property="UpdTime" column="UpdTime" type="DateTime" dbType="DateTime"/>
    </resultMap>
  </resultMaps>
  <statements>

    <select id="SelectDbBackupSet" parameterClass="string" resultClass="DbBackupSet">
      SELECT ID AS Id,Cycle AS Cycle,DayTime AS DayTime,WeekDay AS WeekDay,WeekTime AS WeekTime,MonthDay AS MonthDay,MonthTime AS MonthTime,CreateUser AS CreateUser,CreateTime AS CreateTime,UpdUser AS UpdUser,UpdTime AS UpdTime
      FROM DbBackupSet
      <dynamic prepend="WHERE">
        <isParameterPresent>
          ID = #value#
        </isParameterPresent>
      </dynamic>
    </select>
    <insert id="InsertDbBackupSet" parameterClass="DbBackupSet">
      INSERT INTO DbBackupSet
      (Cycle,DayTime,WeekDay,WeekTime,MonthDay,MonthTime,CreateUser,CreateTime,UpdUser,UpdTime)
      VALUES(#Cycle#,#DayTime#,#WeekDay#,#WeekTime#,#MonthDay#,#MonthTime#,#CreateUser#,#CreateTime#,#UpdUser#,#UpdTime#)
      <selectKey resultClass="long" type="post" property="Id">
        SELECT LAST_INSERT_ROWID() as value
      </selectKey>
    </insert>
    <update id="UpdateDbBackupSet" parameterClass="DbBackupSet">
      UPDATE DbBackupSet
      SET Cycle=#Cycle#,DayTime=#DayTime#,WeekDay=#WeekDay#,WeekTime=#WeekTime#,MonthDay=#MonthDay#,MonthTime=#MonthTime#,CreateUser=#CreateUser#,CreateTime=#CreateTime#,UpdUser=#UpdUser#,UpdTime=#UpdTime#
      WHERE ID = #Id#
    </update>
    <delete id="DeleteDbBackupSet" parameterClass="string">
      DELETE FROM DbBackupSet
      WHERE ID = #value#
    </delete>
  </statements>
</sqlMap>
