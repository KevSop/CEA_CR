<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="PayTypeManageMap"   xmlns="http://ibatis.apache.org/mapping"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="SqlMap.xsd">
  <alias>
    <typeAlias alias="PayTypeManage" assembly="DataAccess.dll" type="DataAccess.Domain.PayTypeManage" />
  </alias>
  <resultMaps>
    <resultMap id="PayTypeManageResult" class="PayTypeManage">
      <result property="Id" column="ID" type="long" dbType="long"/>
      <result property="Code" column="Code" type="string" dbType="varchar"/>
      <result property="Name" column="Name" type="string" dbType="varchar"/>
      <result property="Fee" column="Fee" type="single" dbType="Real"/>
      <result property="IsIncome" column="IsIncome" type="long" dbType="long"/>
      <result property="IsReturnFloat" column="IsReturnFloat" type="long" dbType="long"/>
      <result property="IsSepcialDis" column="IsSepcialDis" type="long" dbType="long"/>
      <result property="IsNeedInputTicNo" column="IsNeedInputTicNo" type="long" dbType="long"/>
      <result property="IsFixedValue" column="IsFixedValue" type="long" dbType="long"/>
      <result property="IsValidate" column="IsValidate" type="long" dbType="long"/>
      <result property="IsFullPay" column="IsFullPay" type="long" dbType="long"/>
      <result property="IsCanUseDis" column="IsCanUseDis" type="long" dbType="long"/>
      <result property="CreateUser" column="CreateUser" type="string" dbType="varchar"/>
      <result property="CreateTime" column="CreateTime" type="DateTime" dbType="DateTime"/>
      <result property="UpdUser" column="UpdUser" type="string" dbType="varchar"/>
      <result property="UpdTime" column="UpdTime" type="DateTime" dbType="DateTime"/>
    </resultMap>
  </resultMaps>
  <statements>

    <select id="SelectPayTypeManage" parameterClass="string" resultClass="PayTypeManage">
      SELECT ID AS Id,Code AS Code,Name AS Name,Fee AS Fee,IsIncome AS IsIncome,IsReturnFloat AS IsReturnFloat,IsSepcialDis AS IsSepcialDis,IsNeedInputTicNo AS IsNeedInputTicNo,IsFixedValue AS IsFixedValue,IsValidate AS IsValidate,IsFullPay AS IsFullPay,IsCanUseDis AS IsCanUseDis,CreateUser AS CreateUser,CreateTime AS CreateTime,UpdUser AS UpdUser,UpdTime AS UpdTime
      FROM PayTypeManage
      <dynamic prepend="WHERE">
        <isParameterPresent>
          ID = #value#
        </isParameterPresent>
      </dynamic>
    </select>
    <select id="SelectPayTypeManageName" parameterClass="string" resultClass="PayTypeManage">
      SELECT ID AS Id,Code AS Code,Name AS Name,Fee AS Fee,IsIncome AS IsIncome,IsReturnFloat AS IsReturnFloat,IsSepcialDis AS IsSepcialDis,IsNeedInputTicNo AS IsNeedInputTicNo,IsFixedValue AS IsFixedValue,IsValidate AS IsValidate,IsFullPay AS IsFullPay,IsCanUseDis AS IsCanUseDis,CreateUser AS CreateUser,CreateTime AS CreateTime,UpdUser AS UpdUser,UpdTime AS UpdTime
      FROM PayTypeManage
      <dynamic prepend="WHERE">
        <isParameterPresent>
          Name = #value#
        </isParameterPresent>
      </dynamic>
    </select>
    <insert id="InsertPayTypeManage" parameterClass="PayTypeManage">
      INSERT INTO PayTypeManage
      (Code,Name,Fee,IsIncome,IsReturnFloat,IsSepcialDis,IsNeedInputTicNo,IsFixedValue,IsValidate,IsFullPay,IsCanUseDis,CreateUser,CreateTime,UpdUser,UpdTime)
      VALUES(#Code#,#Name#,#Fee#,#IsIncome#,#IsReturnFloat#,#IsSepcialDis#,#IsNeedInputTicNo#,#IsFixedValue#,#IsValidate#,#IsFullPay#,#IsCanUseDis#,#CreateUser#,#CreateTime#,#UpdUser#,#UpdTime#)
      <selectKey resultClass="long" type="post" property="Id">
        SELECT LAST_INSERT_ROWID() as value
      </selectKey>
    </insert>
    <update id="UpdatePayTypeManage" parameterClass="PayTypeManage">
      UPDATE PayTypeManage
      SET Code=#Code#,Name=#Name#,Fee=#Fee#,IsIncome=#IsIncome#,IsReturnFloat=#IsReturnFloat#,IsSepcialDis=#IsSepcialDis#,IsNeedInputTicNo=#IsNeedInputTicNo#,IsFixedValue=#IsFixedValue#,IsValidate=#IsValidate#,IsFullPay=#IsFullPay#,IsCanUseDis=#IsCanUseDis#,CreateUser=#CreateUser#,CreateTime=#CreateTime#,UpdUser=#UpdUser#,UpdTime=#UpdTime#
      WHERE ID = #Id#
    </update>
    <delete id="DeletePayTypeManage" parameterClass="string">
      DELETE FROM PayTypeManage
      WHERE ID = #value#
    </delete>
  </statements>
</sqlMap>
