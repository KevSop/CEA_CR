<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="IncomingCallSetMap"  xmlns="http://ibatis.apache.org/mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="SqlMap.xsd">
  <alias>
    <typeAlias alias="IncomingCallSet" assembly="DataAccess.dll" type="DataAccess.Domain.IncomingCallSet" />
  </alias>
  <resultMaps>
    <resultMap id="IncomingCallSetResult" class="IncomingCallSet">
      <result property="Id" column="ID" type="long" dbType="long"/>
      <result property="IsEnable" column="IsEnable" type="long" dbType="long"/>
      <result property="IsAllowShutDown" column="IsAllowShutDown" type="long" dbType="long"/>
      <result property="CallModel" column="CallModel" type="string" dbType="varchar"/>
      <result property="DeviceType" column="DeviceType" type="string" dbType="varchar"/>
      <result property="CallPort" column="CallPort" type="string" dbType="varchar"/>
      <result property="BaudRate" column="BaudRate" type="string" dbType="varchar"/>
      <result property="CreateUser" column="CreateUser" type="string" dbType="varchar"/>
      <result property="CreateTime" column="CreateTime" type="DateTime" dbType="DateTime"/>
      <result property="UpdUser" column="UpdUser" type="string" dbType="varchar"/>
      <result property="UpdTime" column="UpdTime" type="DateTime" dbType="DateTime"/>
    </resultMap>
  </resultMaps>
  <statements>

    <select id="SelectIncomingCallSet" parameterClass="string" resultClass="IncomingCallSet">
      SELECT ID AS Id,IsEnable AS IsEnable,IsAllowShutDown AS IsAllowShutDown,CallModel AS CallModel,DeviceType AS DeviceType,CallPort AS CallPort,BaudRate AS BaudRate,CreateUser AS CreateUser,CreateTime AS CreateTime,UpdUser AS UpdUser,UpdTime AS UpdTime
      FROM IncomingCallSet
      <dynamic prepend="WHERE">
        <isParameterPresent>
          ID = #value#
        </isParameterPresent>
      </dynamic>
    </select>
    <insert id="InsertIncomingCallSet" parameterClass="IncomingCallSet">
      INSERT INTO IncomingCallSet
      (IsEnable,IsAllowShutDown,CallModel,DeviceType,CallPort,BaudRate,CreateUser,CreateTime,UpdUser,UpdTime)
      VALUES(#IsEnable#,#IsAllowShutDown#,#CallModel#,#DeviceType#,#CallPort#,#BaudRate#,#CreateUser#,#CreateTime:DateTime:1/1/0001 12:00:00 AM#,#UpdUser#,#UpdTime:DateTime:1/1/0001 12:00:00 AM#)
      <selectKey resultClass="long" type="post" property="Id">
        SELECT LAST_INSERT_ROWID() as value
      </selectKey>
    </insert>
    <update id="UpdateIncomingCallSet" parameterClass="IncomingCallSet">
      UPDATE IncomingCallSet
      SET IsEnable=#IsEnable#,IsAllowShutDown=#IsAllowShutDown#,CallModel=#CallModel#,DeviceType=#DeviceType#,CallPort=#CallPort#,BaudRate=#BaudRate#,CreateUser=#CreateUser#,CreateTime=#CreateTime#,UpdUser=#UpdUser#,UpdTime=#UpdTime#
      WHERE ID = #Id#
    </update>
    <delete id="DeleteIncomingCallSet" parameterClass="string">
      DELETE FROM IncomingCallSet
      WHERE ID = #value#
    </delete>
  </statements>
</sqlMap>
