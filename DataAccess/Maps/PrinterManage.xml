<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="PrinterManageMap"   xmlns="http://ibatis.apache.org/mapping"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="SqlMap.xsd">
  <alias>
    <typeAlias alias="PrinterManage" assembly="DataAccess.dll" type="DataAccess.Domain.PrinterManage" />
  </alias>
  <resultMaps>
    <resultMap id="PrinterManageResult" class="PrinterManage">
      <result property="Id" column="ID" type="long" dbType="long"/>
      <result property="PrinterNo" column="PrinterNo" type="string" dbType="varchar"/>
      <result property="PrinterName" column="PrinterName" type="string" dbType="varchar"/>
      <result property="IsEnable" column="IsEnable" type="long" dbType="long"/>
      <result property="Status" column="Status" type="string" dbType="varchar"/>
      <result property="CreateUser" column="CreateUser" type="string" dbType="varchar"/>
      <result property="CreateTime" column="CreateTime" type="DateTime" dbType="DateTime"/>
      <result property="UpdUser" column="UpdUser" type="string" dbType="varchar"/>
      <result property="UpdTime" column="UpdTime" type="DateTime" dbType="DateTime"/>
    </resultMap>
  </resultMaps>
  <statements>

    <select id="SelectPrinterManage" parameterClass="string" resultClass="PrinterManage">
      SELECT ID AS Id,PrinterNo AS PrinterNo,PrinterName AS PrinterName,IsEnable AS IsEnable,Status AS Status,CreateUser AS CreateUser,CreateTime AS CreateTime,UpdUser AS UpdUser,UpdTime AS UpdTime
      FROM PrinterManage
      <dynamic prepend="WHERE">
        <isParameterPresent>
          ID = #value#
        </isParameterPresent>
      </dynamic>
    </select>

    <select id="IsExistsPrinterManage" parameterClass="Hashtable" resultClass="Hashtable">
      SELECT ID AS Id,PrinterNo AS PrinterNo,PrinterName AS PrinterName,IsEnable AS IsEnable,Status AS Status,CreateUser AS CreateUser,CreateTime AS CreateTime,UpdUser AS UpdUser,UpdTime AS UpdTime
      FROM PrinterManage
      <dynamic prepend="WHERE">
      <isParameterPresent>
        PrinterName = #value#
      </isParameterPresent>
    </dynamic>
    </select>
    
    <insert id="InsertPrinterManage" parameterClass="PrinterManage">
      INSERT INTO PrinterManage
      (PrinterNo,PrinterName,IsEnable,Status,CreateUser,CreateTime,UpdUser,UpdTime)
      VALUES(#PrinterNo#,#PrinterName#,#IsEnable#,#Status#,#CreateUser#,#CreateTime#,#UpdUser#,#UpdTime#)
      <selectKey resultClass="long" type="post" property="Id">
        SELECT LAST_INSERT_ROWID() as value
      </selectKey>
    </insert>
    <update id="UpdatePrinterManage" parameterClass="PrinterManage">
      UPDATE PrinterManage
      SET PrinterNo=#PrinterNo#,PrinterName=#PrinterName#,IsEnable=#IsEnable#,Status=#Status#,CreateUser=#CreateUser#,CreateTime=#CreateTime#,UpdUser=#UpdUser#,UpdTime=#UpdTime#
      WHERE ID = #Id#
    </update>
    <delete id="DeletePrinterManage" parameterClass="string">
      DELETE FROM PrinterManage
      WHERE ID = #value#
    </delete>
  </statements>
</sqlMap>
